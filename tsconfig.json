{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES5",
    "noImplicitAny": true,
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*"],
      "binmanager/*": ["src/*"],
      "lib/*": ["src/lib/*"]
    },
    "moduleResolution": "Node"
  },
  "include": ["src/**/*", "tests/**/*"]
}
